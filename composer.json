{
    "name": "for/http-middleware-psr15-adapter",
    "description": "PSR15 adapter for react/http middleware",
    "keywords": ["HTTP", "HTTPS", "ReactPHP", "middleware", "PSR15"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/http-server-middleware": "^1.0",
        "react/async": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "react/http": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfReact\\Http\\Middleware\\Psr15Adapter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FriendsOfReact\\Tests\\Http\\Middleware\\Psr15Adapter\\": "tests"
        }
    }
}
